                <TableColumn fx:id="id" prefWidth="44.0" text="ID" />
                <TableColumn fx:id="numcmr" prefWidth="123.0" text="NUMERO CHAMBRE" />
                <TableColumn fx:id="desccmr" prefWidth="286.0" text="DESCRIPTION CHAMBRE" />
                <TableColumn fx:id="dispocmr" prefWidth="96.0" text="DISPONIBILITE" />
            </columns>
        </TableView>
        <GridPane layoutX="118.0" layoutY="36.0">
            <columnConstraints>
