<AnchorPane prefHeight="418.0" prefWidth="635.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.gestionhotel.EspaceEmploye.GestionChambreController">
